{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.astro ./",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then cd .. && husky install client/.husky; fi",
    "precommit": "npm run format && npm run lint:fix"
  },
  "dependencies": {
    "@next/font": "^13.0.6",
    "@nextui-org/react": "^1.0.0-beta.10",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "eslint": "8.28.0",
    "eslint-config-next": "^13.0.6",
    "next": "^13.0.6",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "sass": "^1.56.2",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "tailwindcss": "^3.2.4"
  }
}

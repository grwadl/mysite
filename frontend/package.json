{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"pages/**/*.ts\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.astro pages",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then cd .. && husky install client/.husky; fi",
    "precommit": "npm run format && npm run lint:fix"
  },
  "dependencies": {
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  }
}
